                        token = 'e4bcdab1653d5c2b619e324bedf00925903a7da7';
                        $(this).append('<input type="hidden" name="csrf_token" value="' + token + '"><input type="hidden" name="csrf_page" value="index">');
                    }
                });
                if ($.adblock === undefined) {
                    $.adblock = true;
                }
