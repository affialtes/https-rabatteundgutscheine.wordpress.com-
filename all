                var opened = 0;
                $('a#open-link').click(function () {
                    $('#showBanner').hide();
                    if (opened == 0) {
                        opened1 = 0;
                        $('#showLinks').slideDown(500);
                        opened = 1;
                        return false;
                    } else {
                        $('#showLinks').slideUp(500);
                        opened = 0;
                        return false;
                    }
                });

                var opened1 = 0;
                $('a#open-banner').click(function () {
                    $('#showLinks').hide();
                    if (opened1 == 0) {
                        opened = 0;
                        $('#showBanner').slideDown(500);
                        opened1 = 1;
                        return false;
                    } else {
                        $('#showBanner').slideUp(500);
                        opened1 = 0;
                        return false;
                    }
                });
                $('form').each(function (index, value) { 
