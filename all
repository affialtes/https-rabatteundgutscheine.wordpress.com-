<script type="text/javascript">
jQuery( function( $ ) {
	_tkq = window._tkq || [];
	_tkq.push( [ 'identifyUser', 136943407, 'jasminisel85' ] );
	var postId = 1138;
	var postType = 'jetpack-testimonial';
	var actionOrig = $( '#publishing-action #publish' );

	// Bail on an empty actionOrig; we're in the Gutenberg editor, which doesn't match this selector.
	if ( 0 === actionOrig.length ) {
		return;
	}

	actionOrig = actionOrig.val().toLowerCase();

	$( '#publishing-action #publish' ).on ( 'click', function() {
		var button = this;
		var action = button.value.toLowerCase();
		var postStatus = $( '#post_status' ).val();
		if ( actionOrig == action ) {
			if ( 'pending' == postStatus ) {
				action = 'pending';
			} else {
				action = 'update';
			}
		} else {
			action = 'schedule';
		}

		var eventName = 'wpcom_wpadmineditor_cpt_' + action;
		_tkq.push( [ 'recordEvent', eventName, getEventProps() ] );
	} );

	$( '#preview-action #post-preview' ).on ( 'click', function() {
		var eventName = 'wpcom_wpadmineditor_cpt_preview';
		_tkq.push( [ 'recordEvent', eventName, getEventProps() ] );
	} );

	$( '#save-action #save-post' ).on ( 'click', function() {
		if ( 'pending' == $( '#post_status' ).val() ) {
			action = 'pending';
		} else {
			action = 'savedraft';
		}
		var eventName = 'wpcom_wpadmineditor_cpt_' + action;
		_tkq.push( [ 'recordEvent', eventName, getEventProps() ] );
	} );

	var getVisibility = function() {
		var visibility = 'public';
		if ( $( 'input[name=visibility]:checked' ).val() ) {
			visibility = $( 'input[name=visibility]:checked' ).val() ;
		}
		return visibility;
	}

	var getEventProps = function() {
		var visibility = getVisibility();
		return {
			post_id: postId,
			post_type: postType,
			visibility: visibility
		}
	}
} );
</script>

